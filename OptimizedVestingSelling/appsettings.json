{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.RollingFile"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "logs.txt",
          "retainedFileCountLimit": 14,
          "shared": true,
          "buffered": false,
          "flushToDiskInterval": "00:00:10",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}] [{Level}] {MachineName} ({ThreadId}) <{SourceContext}> {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}] [{Level}] {MachineName} ({ThreadId}) <{SourceContext}> {Message}{NewLine}{Exception}"
        }
      }
    ]
  },
  "NetworkSettings": {
    "ChainId": 137,
    "Name": "Polygon",
    "Symbol": "MATIC",
    "JsonRpcUrl": "https://polygon-rpc.com"
  },
  "ExecutionSettings": {
    "Amount": 5,
    "Type": "Amount",
    "CoinToSellAddress": "0x...",
    "CoinToReceiveAddress": "0x..."
  },
  "WalletSettings": {
    "PublicAddress": "0x4bca9E17a8326d283d1fC68A725B46b42c2D9146",
    "PathToPrivateKey": "C:\\Lab\\C#\\optimized-vesting-selling\\privateKey.pem"
  }
}